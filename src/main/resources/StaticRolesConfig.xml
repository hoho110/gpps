<?xml version="1.0" encoding="UTF-8"?>
<StaticRolesConfig enabled="false">
	<!--匿名用户(未登陆)角色,privilege为-1
		limitedType:受限类型,1:不受限;0:部分许可;-1:全部受限
	-->
	<!-- 匿名用户 -->
	<Role privilege = "-1" limitedType="0">
		<permissionRules>
			<PermissionRule  entityLimitType="gpps.service.ILenderService" operations="register;registerSecondStep;getCurrentUser;login;sendMessageValidateCode"/>
		</permissionRules>
	</Role>
	<!-- 未完成认证的贷款人 -->
	<Role privilege = "0" limitedType="0">
        <permissionRules>
        
        </permissionRules>
    </Role>
    <!-- 完成认证的贷款人 -->
	<Role privilege = "1" limitedType="0">
		<permissionRules>
		</permissionRules>
	</Role>
</StaticRolesConfig>